{
  "name": "obaranovskyi-starter-kit",
  "version": "1.0.0",
  "description": "Test package to go trough the CI flow.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "lint": "eslint '**/*.js'; exit 0",
    "lint:fix": "prettier-eslint $PWD/'src/**/*.js' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obaranovskyi/obaranovskyi-starter-kit.git"
  },
  "keywords": [
    "js",
    "starter-kit"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:fix"
    ]
  },
  "author": "Oleh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/obaranovskyi/obaranovskyi-starter-kit/issues"
  },
  "homepage": "https://github.com/obaranovskyi/obaranovskyi-starter-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^2.40.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
